<% if (Array.isArray(_outcome.outcome.impediments.impediment)) { %>
    <% _outcome.outcome.impediments.impediment.forEach(function(imped, i) { %> 
        <% var status = imped.$.status.toLowerCase(); %>
        <% if ((status == "green") || (status == "yellow") || (status == "red")) { %>
            <div class="row">
                <div class="col-12 imped-pad">
                    <% if (status == "green") { %>
                    <div class="imped-color-success">
                    <% } else if (status == "yellow") { %>
                    <div class="imped-color-warning">
                    <% } else if (status == "red") { %>
                    <div class="imped-color-dire">
                    <% } %>

                        <div class="row">
                            <div class="col-2 imped-status-col">
                                <div class="row">
                                    <div class="col-2">
                                        <div class="imped-title"> 
                                            <h5>Status</h5>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="offset-md-1"></div>
                                    <div class="col-2">
                                        <% if (status == "green") { %>
                                        <div class="imped-color-success-icon">
                                        <% } else if (status == "yellow") { %>
                                        <div class="imped-color-warning-icon">
                                        <% } else if (status == "red") { %>
                                        <div class="imped-color-dire-icon">
                                        <% } %>
                                        <% if ((status == "green") || (status == "yellow") || (status == "red")) {%>
                                        </div>
                                        <% } %>
                                        <% if (1 == 0) { %> </div></div> <% } %>
                                    </div>
                                </div>
                            </div>
                            <div class="col-7 imped-desc-col">
                                <div class="imped-content">
                                    <div class="row imped-title">
                                        <h5> <%= imped.$.name %> </h5>
                                    </div>
                                    <div class="row imped-desc">
                                            <%= imped.desc %>
                                    </div>
                                </div>
                            </div>
                            <div class="col-2 imped-status-col">
                                <div class="row impediment-title">
                                    Raised to
                                </div>
                                <div class="row">
                                    <div class="card slt-card">
                                        <img class="card-img-top slt-img" src="" alt="<%=_outcome.outcome.impediments.impediment[i].$.raisedto%>">
                                        <div class="card-body slt-name">
                                            <p class="card-text" ><%= imped.$.raisedto%> </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-1.5 imped-desc-col">
                                <div class="row impediment-title">
                                    Raised by
                                </div>
                                <div class="row">
                                    <div class="card slt-card">
                                            <img class="card-img-top slt-img" src="" alt="<%=_outcome.outcome.impediments.impediment[i].$.raisedby%>">
                                        <div class="card-body slt-name">
                                            <p class="card-text" ><%= imped.$.raisedby%> </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    <% if ((imped.$.status == "red") || (imped.$.status == "yellow") || (imped.$.status == "green")) { %>
                    </div>
                    <% } %>
                    <!-- for div closing tags when using text editors: ignore -->
                    <% if (1 == 0) { %>
                    </div></div>
                    <% } %>
                </div>
            </div>
        <% } %>
    <% }); %>
<% } else { %>
    <% var status = _outcome.outcome.impediments.impediment.$.status; %>
    <% if ((status == "green") || (status == "yellow") || (status == "red")) { %>
        <div class="row">
            <div class="col-12 imped-pad">
                <% if (status == "green") { %>
                <div class="imped-color-success">
                <% } else if (status == "yellow") { %>
                <div class="imped-color-warning">
                <% } else if (status == "red") { %>
                <div class="imped-color-dire">
                <% } %>
                    <div class="row">
                        <div class="col-2 imped-status-col">
                            <div class="row">
                                <div class="col-2">
                                    <div class="imped-title"> 
                                        <h5>Status</h5>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="offset-md-1"></div>
                                <div class="col-2" id="ex_i1_status">
                                    <% if (status == "green") { %>
                                    <div class="imped-color-success-icon">
                                    <% } else if (status == "yellow") { %>
                                    <div class="imped-color-warning-icon">
                                    <% } else if (status == "red") { %>
                                    <div class="imped-color-dire-icon">
                                    <% } %>
                                    <% if ((status == "green") || (status == "yellow") || (status == "red")) {%>
                                    </div>
                                    <% } %>
                                    <% if (1 == 0) { %> </div></div> <% } %>
                                </div>
                            </div>
                        </div>
                        <div class="col-7 imped-desc-col">
                            <div class="imped-content">
                                <div class="row imped-title">
                                    <h5> <%= _outcome.outcome.impediments.impediment.$.name %> </h5>
                                </div>
                                <div class="row imped-desc">
                                        <%= _outcome.outcome.impediments.impediment.desc %>
                                </div>
                            </div>
                        </div>
                        <div class="col-2 imped-status-col">
                            <div class="row impediment-title">
                                Raised to
                            </div>
                            <div class="row">
                                <div class="card slt-card">
                                    <img class="card-img-top slt-img" src="" alt="<%=_outcome.outcome.impediments.impediment.$.raisedto%>">
                                    <div class="card-body slt-name">
                                        <p class="card-text" ><%= _outcome.outcome.impediments.impediment.$.raisedto%> </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-1.5 imped-desc-col">
                            <div class="row impediment-title">
                                Raised by
                            </div>
                            <div class="row">
                                <div class="card slt-card">
                                        <img class="card-img-top slt-img" src="" alt="<%=_outcome.outcome.impediments.impediment.$.raisedby%>">
                                    <div class="card-body slt-name">
                                        <p class="card-text" ><%= _outcome.outcome.impediments.impediment.$.raisedby%> </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                <% if ((_outcome.outcome.impediments.impediment.$.status == "red") || (_outcome.outcome.impediments.impediment.$.status == "yellow") || (_outcome.outcome.impediments.impediment.$.status == "green")) { %>
                </div>
                <% }; %>
                <!-- for div closing tags when using text editors: ignore -->
                <% if (1 == 0) { %>
                </div></div>
                <% } %>
            </div>
        </div>
    <% }; %>
<% }; %>